<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>                     <%# i %>
    <%= render user %>                            <%# i %>
    <p>
      <%= link_to "Show this user", user %>       <%# i %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path %>
